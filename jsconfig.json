
{
	"compilerOptions": {
		"lib": [
			"DOM",
			"ESNext",
			"WebWorker"
		],
		"module": "ESNext",
		"moduleResolution": "node",
		"checkJs": true,
		"esModuleInterop": true,
		"alwaysStrict": true,
		"sourceMap": true,
		"target": "ESNext",
		"allowUnreachableCode": false,
		"noImplicitAny": false,
		"strictNullChecks": false,
		"noImplicitReturns": true,
		"noUncheckedIndexedAccess": false,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"types": [
			"node",
			"dom",
			"acorn"
		],
		"outDir": "dist"
	},
	"exclude": [
		"node_modules/**/*"
	],
	"include": [
		"./src/**/*",
		"serviceWorker.js"
	]
}